{
  "name": "reactive-battle",
  "version": "1.0.0",
  "description": "A new interpretation of the famous Battle City game",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shchegol/reactive-battle.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shchegol/reactive-battle/issues"
  },
  "homepage": "https://github.com/shchegol/reactive-battle#readme",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy",
      "^@root(.*)$": "<rootDir>/src/$1",
      "^@components(.*)$": "<rootDir>/src/components/$1",
      "^@styles(.*)$": "<rootDir>/src/styles/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run test"
    }
  },
  "browserslist": [
    "> .5% and last 2 versions",
    "not dead",
    "not OperaMini all",
    "ie >= 11",
    "Edge >= 12"
  ],
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.13",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "autoprefixer": "10.1.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "eslint": "7.15.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "1.0.0",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.3.3",
    "node-sass": "5.0.0",
    "postcss": "8.2.1",
    "postcss-loader": "4.1.0",
    "resolve-url-loader": "3.1.2",
    "sass": "1.30.0",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "8.0.12",
    "typescript": "4.1.3",
    "webpack": "5.10.1",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.7.0"
  },
  "dependencies": {
    "express": "4.17.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "engines": {
    "node": ">=14.13.0"
  }
}