{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "es2017": true,
    "jest": true
  },
  "extends": [
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0
  }
}